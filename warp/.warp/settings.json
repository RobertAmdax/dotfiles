{
  "theme": {
    "mode": "dark",
    "name": "Catppuccin Mocha"
  },
  "font": {
    "size": 16,
    "family": "CaskaydiaCove Nerd Font Mono"
  },
  "window": {
    "opacity": 0.92,
    "blur": true,
    "vibrancy": true,
    "padding": 8
  },
  "terminal": {
    "shell": "/bin/zsh",
    "scrollback_lines": 10000,
    "confirm_quit_with_running_processes": true
  },
  "ai": {
    "enabled": true,
    "suggestions": {
      "enabled": true,
      "inline": true
    },
    "command_generation": {
      "enabled": true
    },
    "explanation": {
      "enabled": true
    }
  },
  "features": {
    "command_palette": true,
    "workflows": true,
    "blocks": true,
    "ssh_wrapper": true,
    "mouse_reporting": true
  },
  "keybindings": {
    "copy": "cmd+c",
    "paste": "cmd+v",
    "new_tab": "cmd+t",
    "close_tab": "cmd+w",
    "next_tab": "cmd+shift+]",
    "previous_tab": "cmd+shift+[",
    "split_pane_right": "cmd+d",
    "split_pane_down": "cmd+shift+d",
    "command_palette": "cmd+p",
    "search": "cmd+f",
    "clear_screen": "cmd+k"
  },
  "appearance": {
    "tab_bar": {
      "visible": true,
      "position": "top"
    },
    "session_header": {
      "visible": true
    },
    "input_line": {
      "position": "bottom"
    }
  },
  "integrations": {
    "honor_ps1": false,
    "auto_suggestions_strategy": "ai_enhanced"
  }
}