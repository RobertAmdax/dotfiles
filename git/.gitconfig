[user]
    name = Robert Bakker
    email = your-email@example.com

[core]
    editor = nvim
    autocrlf = false
    filemode = false
    pager = delta

[init]
    defaultBranch = main

[pull]
    rebase = true

[push]
    default = simple
    autoSetupRemote = true

[merge]
    tool = nvim
    conflictstyle = diff3

[diff]
    tool = nvim
    colorMoved = default

[delta]
    navigate = true
    side-by-side = true
    line-numbers = true
    syntax-theme = Catppuccin-mocha

[interactive]
    diffFilter = delta --color-only

[alias]
    st = status
    co = checkout
    br = branch
    ci = commit
    ca = commit -a
    cm = commit -m
    cam = commit -am
    unstage = reset HEAD --
    last = log -1 HEAD
    visual = !gitk
    tree = log --oneline --decorate --all --graph
    undo = reset HEAD~1 --mixed
    amend = commit -a --amend
    count = !git shortlog -sn
    ignored = !git ls-files -v | grep "^[[:lower:]]"
